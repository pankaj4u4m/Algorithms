#include<cstdio>
using namespace std;

long long getMatchCount(int n,  int m,  int inv[],  nt bill[]){

}

int main(){
	int n, m;
	scanf("%d%d", &n, &m);
	int inv[n],  bill[m];
	for(int i = 0; i< n; ++i){
		scanf("%d",  &inv[i]);
	}
	for(int i = 0; i< m; ++i){
		scanf("%d", &bill[i]);
	}

	long long cnt = getMatchCount(n, m, inv, bill);
	printf("%d\n", cnt);
	return 0;
}
